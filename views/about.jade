extends layout

block content
  div.section.animated.fadeInUp
    h1= title.text
  div.section.light-bg.animated.fadeInUp
    div.container.inner
      div.row
        div.col-md-4
          img(src='#{image}')
        div.col-md-8
          for val in paragraphs
            div.paragraph
              h2= val.heading
              p= val.content